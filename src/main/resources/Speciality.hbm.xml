<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.javabegin.training.springlibrary.entities.Speciality" table="speciality" schema="public"
           catalog="ivansorochinsky">
        <property name="id">
            <column name="id" sql-type="integer" not-null="true"/>
        </property>
        <property name="denomination">
            <column name="denomination" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="countStudent">
            <column name="countStudent" sql-type="integer" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>