<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.javabegin.training.springlibrary.entities.Educator" table="educator" schema="public"
           catalog="ivansorochinsky">
        <property name="id">
            <column name="id" sql-type="integer" not-null="true"/>
        </property>
        <property name="firstname">
            <column name="firstname" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="lastname">
            <column name="lastname" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="kafedra">
            <column name="kafedra" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="position">
            <column name="position" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="workload">
            <column name="workload" sql-type="integer" not-null="true"/>
        </property>
        <property name="timetableOfWork">
            <column name="timetableOfWork" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>